@{
	ViewBag.Title = "Index";
}
<h3>@ViewBag.Message</h3>

<table style="width: 100%" border="1">
	@foreach (var item in ViewBag.Products)
	{
	<tr>
		<td>@item.Id</td>
		<td>@item.Name</td>
		<td>@item.Description</td>
		<td>@item.Category</td>
		<td>@item.Producer</td>
		<td>@item.Supplier</td>
		<td>@item.Price</td>
	</tr>
	}
</table>

